<view class="zsy_calendar"><view class="calendar_info"><text class="title">运动记录</text><text class="desc">{{'('+($root.m0===$root.m1?'':$root.m2+'年')+$root.m3+'月)'}}</text><text data-event-opts="{{[['tap',[['goToDate']]]]}}" hidden="{{!(showBackToTodayBtn)}}" class="backToToday" style="{{'color:'+(dateActiveColor)+';'}}" bindtap="__e">回到今天</text></view><view class="calendar_week"><block wx:for="{{week}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="calendar_week__item">{{item}}</view></block></view><view class="calendar_swiper"><swiper style="{{'height:'+(swiperHeight)+';'}}" circular="{{true}}" current="{{current}}" duration="{{duration}}" skip-hidden-item-layout="{{true}}" data-event-opts="{{[['change',[['e0',['$event']]]]]}}" bindchange="__e"><block wx:for="{{3}}" wx:for-item="swiper" wx:for-index="swiperIndex" wx:key="swiperIndex"><swiper-item class="swiper-item"><date-box vue-id="{{'3530d581-1-'+swiperIndex}}" dates="{{getcurCalendarDates[swiperIndex]}}" cellHeight="{{cellHeight}}" selectedDate="{{selectedDate}}" dateActiveColor="{{dateActiveColor}}" swiperMode="{{swiperMode}}" showActive="{{emitTimer===null}}" markedDates="{{markedDates}}" data-event-opts="{{[['^chooseDate',[['chooseDate']]]]}}" bind:chooseDate="__e" bind:__l="__l"></date-box></swiper-item></block></swiper></view><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="calendar_toggle" bindtap="__e"><view class="{{['icon',(swiperMode==='close')?'down':'']}}"></view></view></view>