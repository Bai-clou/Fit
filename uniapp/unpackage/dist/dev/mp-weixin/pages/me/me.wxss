
.container{
	/* height: 100%; */
	width: 100%;
	position: absolute;
	top: 10%;
	background-color:#fcfcfc;
	border-top-right-radius: 20px;
	border-top-left-radius: 20px;
}
.three{
	display: flex;
	flex-direction: column;
	align-items: center;
	padding-bottom: 10rpx;
}
.three > text:nth-child(1){
	color: #a7a7a7;
	font-size: 14px;
}
.three > text:nth-child(2){
	font-size: 16px;
	font-weight: bold;
}
.inform > text{
	padding: 10rpx;
}
.cards{
	display: flex;
	justify-content: space-around;
}
.card{
	display: flex;
	flex-direction: row;
	padding: 20rpx;
}
.threeView > view{
	padding-top: 10rpx;
}
.logo > view, 
.logo > image{
	padding: 20rpx;
}


